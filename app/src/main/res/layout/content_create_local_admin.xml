<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#34eb9c"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <EditText
        android:id="@+id/editText200"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:background="#ffffff"
        android:ems="10"
        android:gravity="center"
        android:hint="F name"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText201"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:background="#ffffff"
        android:ems="10"
        android:gravity="center"
        android:hint="L name"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/editText200"
        app:layout_constraintTop_toBottomOf="@+id/editText200" />

    <EditText
        android:id="@+id/editText202"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="35dp"
        android:background="#ffffff"
        android:ems="10"
        android:gravity="center"
        android:hint="CNIC"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/editText201"
        app:layout_constraintTop_toBottomOf="@+id/editText201" />

    <EditText
        android:id="@+id/editText204"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:background="#ffffff"
        android:ems="10"
        android:gravity="center"
        android:hint="Admin ID"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/editText203"
        app:layout_constraintTop_toBottomOf="@+id/editText203" />

    <EditText
        android:id="@+id/editText205"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:background="#ffffff"
        android:ems="10"
        android:gravity="center"
        android:hint="Region"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/editText204"
        app:layout_constraintTop_toBottomOf="@+id/editText204" />

    <EditText
        android:id="@+id/editText206"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:background="#ffffff"
        android:ems="10"
        android:gravity="center"
        android:hint="Password"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/editText205"
        app:layout_constraintTop_toBottomOf="@+id/editText205" />

    <EditText
        android:id="@+id/editText207"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:background="#ffffff"
        android:ems="10"
        android:gravity="center"
        android:hint="Re enter password"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/editText206"
        app:layout_constraintTop_toBottomOf="@+id/editText206" />

    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="Create"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText207" />

    <EditText
        android:id="@+id/editText203"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:background="#ffffff"
        android:ems="10"
        android:gravity="center"
        android:hint="Mobile No."
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/editText202"
        app:layout_constraintTop_toBottomOf="@+id/editText202" />
</androidx.constraintlayout.widget.ConstraintLayout>